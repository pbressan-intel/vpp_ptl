set ip neighbor eth7 192.168.6.2 00:e0:ed:fa:13:a5
set ip neighbor eth0 10.10.6.2 6c:fe:54:47:49:b9
set ip neighbor eth0 10.10.6.3 6c:fe:54:47:4d:00
set ip neighbor eth0 10.10.6.4 6c:fe:54:47:67:89

set interface ip address eth7 192.168.6.1/24
set interface ip address eth0 10.10.6.1/24

set interface state eth7 up
set interface state eth0 up

set int l2 bridge eth7 1
set int l2 bridge eth0 1

l4fw eth7
l4fw eth0
